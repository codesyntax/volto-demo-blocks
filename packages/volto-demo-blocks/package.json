{
  "name": "volto-demo-blocks",
  "version": "1.0.0-alpha.0",
  "description": "An addon for Volto thatt contains some dummy blocks. The code on this block is a demo you can copy to your project. Its aim is to show how simple blocks are built using components provided by Volto",
  "main": "src/index.js",
  "license": "MIT",
  "keywords": [
    "volto-addon",
    "volto",
    "plone",
    "react"
  ],
  "author": "Mikel Larreategi",
  "homepage": "https://github.com/codesyntax/volto-demo-blocks#readme",
  "repository": {
    "type": "git",
    "url": "git@github.com:codesyntax/volto-demo-blocks.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "i18n": "rm -rf build/messages && NODE_ENV=production i18n --addon",
    "dry-release": "release-it --dry-run",
    "release": "release-it",
    "release-major-alpha": "release-it major --preRelease=alpha",
    "release-alpha": "release-it --preRelease=alpha"
  },
  "dependencies": {
    "@kitconcept/volto-light-theme": "6.0.0-alpha.9"
  },
  "addons": [
    "@kitconcept/volto-light-theme",
    "volto-demo-blocks"
  ],
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@plone/scripts": "^3.6.1",
    "release-it": "^17.1.1"
  }
}
